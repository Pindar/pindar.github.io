{
  "name": "itnotes",
  "version": "1.0.0",
  "description": "This blog is about it notes, lean development, DevOps and programing",
  "author": "Simon Dittlmann",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@bazzite/nuxt-optimized-images": "^0.2.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/sitemap": "^1.2.0",
    "glob": "^7.1.4",
    "highlight.js": "^10.4.1",
    "markdown-it-classy": "^0.2.0",
    "nuxt": "^2.0.0",
    "tachyons": "^4.11.1",
    "vue-disqus": "^3.0.5",
    "vue-tweet-embed": "^2.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "frontmatter-markdown-loader": "^1.8.0",
    "husky": "^2.6.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4",
    "svg-inline-loader": "^0.8.0",
    "vue-jest": "^4.0.0-0",
    "webp-loader": "^0.5.0"
  }
}
